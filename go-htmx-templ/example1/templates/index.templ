package templates

templ Index(title string) {
    <html>
        <head>
            <title>{ title }</title>
            <meta charset="UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
        </head>
        <body>
            @Hello()
        </body>
    </html>
}

templ Hello() {
    <h1>Hello, world!</h1>
}